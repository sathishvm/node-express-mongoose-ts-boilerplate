{
  "name": "node-express-mongoose-ts-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for building production-ready RESTful APIs using Node.js, Express, and Mongoose with Typescript",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/server.ts",
    "start:prod": "NODE_ENV=production nodemon build/server.js",
    "build": "tsc -p ./tsconfig.json && cp -r ./src/views ./build",
    "tsc:w": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sathishvm/node-express-mongoose-ts-boilerplate.git"
  },
  "keywords": [
    "nodejs-typescript",
    "mongoose-typescript",
    "express-typescript",
    "nodejs-typescript-boilerplate",
    "nodejs-typescript-template"
  ],
  "author": "SathishBublu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sathishvm/node-express-mongoose-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/sathishvm/node-express-mongoose-ts-boilerplate#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.1",
    "@types/html-to-text": "^5.1.2",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.2.3",
