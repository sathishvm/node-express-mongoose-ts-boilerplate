    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.17",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "pug": "^3.0.0",
    "validator": "^13.1.17",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  }
}
